{
  "name": "autocrop-worker",
  "version": "1.0.2",
  "description": "A simple, fast autocrop library which automagically crops the image. The cropping related functions are based on based on autocrop.js. This library offloads image processing work from mainthread to workers using workerpool.",
  "main": "dist/autocrop-worker.cjs.js",
  "module": "dist/autocrop-worker.esm.js",
  "browser": "dist/autocrop-worker.umd.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w"
  },
  "keywords": [
    "autocrop",
    "crop",
    "worker",
    "autocropjs"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/JohnPremKumar/autocrop-worker.git"
  },
  "author": "John Prem Kumar S",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "rollup": "^2.33.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.14",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "workerpool": "^6.0.3"
  }
}
